@Metadata.layer: #CUSTOMER
@UI: { headerInfo: { typeName: 'Travel1',
                     typeNamePlural: 'Travel2',
                     title:{ type: #STANDARD,
                             value: 'IncidentId' }
                     },
       presentationVariant: [{ sortOrder: [{ by: 'incidentid',
                                             direction: #DESC }],
                               visualizations: [{ type: #AS_LINEITEM }]
                             }]
      }
annotate entity ZINCT_C_JRZ with
{


  @UI.facet: [{ id: 'IncidentMasterData',
                purpose: #STANDARD,
                type: #IDENTIFICATION_REFERENCE,
                label: 'Incident Master Data',
                position: 10 }]
  @UI.hidden: true
  IncUuid;




  @UI: { lineItem: [{ position: 10,
                    importance: #HIGH,
                    label: 'Incident ID' },
                    { type: #FOR_ACTION,
                      dataAction: 'acceptIncident',
                      label: 'Accept Incident',
                      position: 10 },
                    { type: #FOR_ACTION,
                      dataAction: 'rejectIncident',
                      label: 'Reject Incident',
                      position: 20 },
                  { type: #FOR_ACTION,
                    dataAction: 'changeStatus',
                    label: 'Change Status',
                    position: 30 }
                  ]
                  ,

          selectionField: [{ position: 10 }],
          identification: [{ position: 10 },

                            { type: #FOR_ACTION,
                              dataAction: 'acceptIncident',
                              label: 'Accept Incident',
                              position: 10 },
                            { type: #FOR_ACTION,
                              dataAction: 'rejectIncident',
                              label: 'Reject Incident',
                              position: 20 },
                          { type: #FOR_ACTION,
                            dataAction: 'changeStatus',
                            label: 'Change Status',
                            position: 30 }
                          ]
                           }

  IncidentId;


  @UI: { lineItem: [{ position: 20,
                      importance: #LOW,
                      label: 'Titulo' }],
         selectionField: [{ position: 20 }],
         identification: [{ position: 20 }]
  }
  Title;

  @UI: { lineItem: [{ position: 30,
                    importance: #LOW,
                    label: 'Descripci√≥n' }],
       selectionField: [{ position: 30 }],
       identification: [{ position: 30 }]
  }
  Description;


  @UI: { lineItem: [{ position: 40,
                  importance: #LOW,
                  label: 'Estatus' }],
     selectionField: [{ position: 40 }],
     identification: [{ position: 40 }]
  }
  Status;


  @UI: { lineItem: [{ position: 50,
                importance: #LOW,
                label: 'Prioridad' }],
   selectionField: [{ position: 50 }],
   identification: [{ position: 50 }]
  }
  Priority;

  @UI: { lineItem: [{ position: 60,
              importance: #LOW,
              label: 'Creado el.' }],
  selectionField: [{ position: 60 }],
  identification: [{ position: 60 }]
  }
  Creationdate;

  @UI: { lineItem: [{ position: 70,
            importance: #LOW,
            label: 'Modificado el.' }],
  selectionField: [{ position: 70 }],
  identification: [{ position: 70 }]
  }
  Changedate;



  @UI.hidden: true
  LocalLastChangedAt;

  @UI.hidden: true
  LastChangedAt;


}

